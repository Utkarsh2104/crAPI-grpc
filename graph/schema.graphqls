type Post {
  id: String!
  title: String!
  content: String!
  author: String!
  comments: [String!]
  author_id: String!
  created_at: String! 
}

input PostInput {
  id: String!
  title: String!
  content: String!
  author: String!
  comments: [String!]
  author_id: String!
  created_at: String! 
}

type Coupon {
  coupon_code: String!
  amount: String!
  created_at: String!
}

input CouponInput {
  coupon_code: String!
  amount: String!
  created_at: String!
}

type User {
  id: String!
  nickname: String!
  email: String!
  vehicle_id: String!
  picurl: String!
  created_at: String!
}

input UserInput {
  id: String!
  nickname: String!
  email: String!
  vehicle_id: String!
  picurl: String!
  created_at: String!
}

type Comment {
  id: String!
  content: String!
  created_at: String!
  author: String!
}

input CommentInput {
  id: String!
  content: String!
  created_at: String!
  author: String!
}
