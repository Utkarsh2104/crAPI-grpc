type Query {
  GetPosts(ids: [String!]!): [Post!]!
  GetCoupons(codes: [String]!): [String!]!
}

type Mutation {
  CreatePost(id:String): Boolean!
  UpdatePost(id:String!, input:PostInput!): Boolean!
  DeletePost(postsID:[String!]): [String!]!
  
